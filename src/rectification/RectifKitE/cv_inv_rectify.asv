function [IL, R] = cv_rectify(JL, HL, JR, HR)
% Derectification for two Images

% find the smallest bb containining both images
bb = mcbb(size(IL),size(IR), inv(HL), inv(HR));

% warp RGB channels,
for c = 1:size(IL, 3)

    % Warp LEFT
    [JL(:,:,c),~,~] = my_imwarp(IL(:,:,c), TL, 'bilinear', bb);

    % Warp RIGHT
    [JR(:,:,c),~,~] = my_imwarp(IR(:,:,c), TR, 'bilinear', bb);

end

